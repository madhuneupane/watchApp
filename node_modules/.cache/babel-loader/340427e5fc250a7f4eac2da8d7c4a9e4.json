{"ast":null,"code":"import { db } from \"@/firebase\";\nimport { addDoc, collection } from \"firebase/firestore\";\nexport default {\n  name: \"createAccount\",\n  data() {\n    return {\n      email: \"\",\n      fname: \"\",\n      lname: \"\",\n      password: \"\",\n      cpassword: \"\",\n      fillMessage: \"\"\n    };\n  },\n  methods: {\n    newAccount() {\n      if (this.email == \"\" || this.password == \"\" || this.cpassword == \"\" || this.fname == \"\" || this.lname == \"\") {\n        this.fillMessage = \"Please fill in all the information\";\n      }\n      if (this.password !== this.cpassword) {\n        alert(\"Password and confirm password are not matched\");\n      }\n      try {\n        const docRef = addDoc(collection(db, \"user\"), {\n          email: this.email,\n          password: this.password,\n          fname: this.fname,\n          lname: this.lname\n        });\n        console.log(\"Document written with ID: \", docRef.id);\n      } catch (e) {\n        console.error(\"Error adding document: \", e);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAsCA,SAASA,EAAC,QAAS,YAAY;AAC/B,SAASC,MAAM,EAAEC,UAAS,QAAS,oBAAoB;AACvD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,IACE,IAAI,CAACP,KAAI,IAAK,EAAC,IACf,IAAI,CAACG,QAAO,IAAK,EAAC,IAClB,IAAI,CAACC,SAAQ,IAAK,EAAC,IACnB,IAAI,CAACH,KAAI,IAAK,EAAC,IACf,IAAI,CAACC,KAAI,IAAK,EAAC,EACf;QACA,IAAI,CAACG,WAAU,GAAI,oCAAoC;MACzD;MACA,IAAI,IAAI,CAACF,QAAO,KAAM,IAAI,CAACC,SAAS,EAAE;QACpCI,KAAK,CAAC,+CAA+C,CAAC;MACxD;MACA,IAAI;QACF,MAAMC,MAAK,GAAIb,MAAM,CAACC,UAAU,CAACF,EAAE,EAAE,MAAM,CAAC,EAAE;UAC5CK,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBG,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBF,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,KAAK,EAAE,IAAI,CAACA;QACd,CAAC,CAAC;QAEFQ,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEF,MAAM,CAACG,EAAE,CAAC;MACtD,EAAE,OAAOC,CAAC,